<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <package name="schoolmajor" namespace="/" extends="struts-default">
        <action name="*SM" class="com.zhnjy.action.SchoolMajorAction" method="{1}">
        
           <result name="addsuccess">/manage_jsp/school_setmajor.jsp</result>
           <result name="addfail">/manage_jsp/fail.jsp </result> 
           
           <result name="querysuccess">/manage_jsp/school_setmajor.jsp </result>
           <result name="queryfail">/manage_jsp/fail.jsp </result>
           
            <result name="deletesuccess">/manage_jsp/success_result.html </result>
           <result name="deletefail">/manage_jsp/fail.jsp </result>
        </action>
    </package>

</struts>
