<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <package name="grade" namespace="/" extends="struts-default">
        <action name="*Grade" class="com.zhnjy.action.GradeAction" method="{1}">
        
           <result name="addsuccess">/manage_jsp/success.jsp </result>
           <result name="addfail">/manage_jsp/fail.jsp </result> 
           <result name="getgradessuccess">/show_data_jsp/get_grade.jsp </result>
          
        </action>
    </package>

</struts>
